                                    {$ITEM_HEADER_IMAGE[$j][$i]}&nbsp;<b><a id='{$ID_TAB[$j][$i]}' href='{$ITEM_URL[$j][$i]}' class="tabDetailViewDL2Link">{$ITEM_HEADER_LABEL[$j][$i]}</a></b>
                                    {$ITEM_DESCRIPTION[$j][$i]}
                                </li>
                            {else}
                            {/if}
                   {/if}
                   {assign var='i' value=$i+1}

                   {/foreach}
                </ul>
            </div>
        {/foreach}
    </ul>
    </div>
</div>


